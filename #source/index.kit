               <!--РАДИО-КНОПКА-->
               <div class="options">
                  <div class="form-block__item">
                     <div class="option active">
                        Новинки!
                        <input type="radio" value="1" name="form[type]" />
                     </div>
                     <div class="option">
                        Новинки 2
                        <input type="radio" value="2" name="form[type]" />
                     </div>
                  </div>
               </div>
               <!--form-block - это класс для разделения общий. 
                  его для форм пишем обязательно-->
               <div class="form-block__item">
                  <!--selected завязан с JS-->
                  <select name="form[]" class="form-block">
                     <option value="1" selected="selected">Пункт №1</option>
                     <option value="2">Пункт №2</option>
                     <option value="3">Пункт №3</option>
                     <option value="4">Пункт №4</option>
