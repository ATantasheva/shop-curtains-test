                        <div class="cell">
                           <input type="text" name="form[]" id="rangeto" />
                        </div>
                     </div>
                     <div id="range" class="category-params-item-price-range"></div>
                  </div>
               </div>
               <!--РЕЙТИНГ со звездами -->
